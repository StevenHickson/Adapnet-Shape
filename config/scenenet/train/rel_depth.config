num_gpus: 1
model: 'AdapNet_pp'
dataset_name: 'scenenet'
input_modality: 'rgb'
output_modality:
  - 'relative_depth'
num_classes:
  - 1
orig_intialize:  'init_checkpoint/AdapNet_pp_init.ckpt'
checkpoint: 'checkpoints/scenenet/rel_depth/'
summary_dir: 'graphs/scenenet/rel_depth/'
train_data: './scenenet_train.txt'
batch_size: 32
skip_step: 5
height: 240
width: 320
max_iteration: 150000
learning_rate: 0.001
save_step: 2000
summaries_step: 400
power: 0.9
