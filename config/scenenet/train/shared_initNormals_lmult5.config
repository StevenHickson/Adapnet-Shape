num_gpus: 1
model: 'AdapNet_shared'
dataset_name: 'scenenet'
input_modality: 'rgb'
output_modality:
  - 'labels'
  - 'normals'
num_classes:
  - 14
  - 3
intialize:  'checkpoints/scenenet/normals2/model.ckpt-99999'
checkpoint: 'checkpoints/scenenet/shared_n_5/'
summary_dir: 'graphs/scenenet/shared_n_5/'
train_data: 'scenenet_train.txt'
aux_loss_mode: 'both'
batch_size: 16
skip_step: 5
height: 240
width: 320
max_iteration: 150000
learning_rate: 0.001
save_step: 10000
summaries_step: 400
power: 0.9
